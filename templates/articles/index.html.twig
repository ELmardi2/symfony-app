{% extends 'base.html.twig' %}
{% block body %}
{# Header Page title #}
<h1 class="text-center">Welcome To Articles Page </h1>
{% for article in articles %}
<div class="card p-4">
<div class="card-header">
<h3>{{ article.title }}</h3>
</div>
<div class="card-body">
{{ article.image }}
</div>
<div class="card-body">
